<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.AccountListItemController">
   <children>
      <HBox fx:id="productListItem" alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="967.0">
        <children>
          <Label fx:id="username" prefHeight="25.0" prefWidth="182.0" text="Admin">
            <font>
              <Font size="16.0" />
            </font>
          </Label>
          <Label fx:id="email" prefHeight="25.0" prefWidth="275.0" text="admin@gmail.com">
            <font>
              <Font size="16.0" />
            </font>
          </Label>
          <Label fx:id="password" prefHeight="25.0" prefWidth="276.0" text="password">
            <font>
              <Font size="16.0" />
            </font>
          </Label>
          <Label fx:id="type" prefHeight="25.0" prefWidth="83.0" text="ADMIN">
            <font>
              <Font size="16.0" />
            </font>
          </Label>
          <HBox alignment="CENTER" prefHeight="100.0" prefWidth="140.0" spacing="10.0" HBox.hgrow="ALWAYS">
            <children>
              <ImageView fx:id="view" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#viewAccount" pickOnBounds="true" preserveRatio="true">
                <image>
                  <Image url="@../images/ic_eye.png" />
                </image>
              </ImageView>
              <ImageView fx:id="edit" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#editAccount" pickOnBounds="true" preserveRatio="true">
                <image>
                  <Image url="@../images/ic_pencil.png" />
                </image>
              </ImageView>
              <ImageView fx:id="delete" fitHeight="25.0" fitWidth="25.0" onMouseClicked="#deleteAccount" pickOnBounds="true" preserveRatio="true">
                <image>
                  <Image url="@../images/ic_trash.png" />
                </image>
              </ImageView>
            </children>
          </HBox>
        </children>
        <padding>
          <Insets left="10.0" />
        </padding>
      </HBox>
      <Separator prefWidth="200.0" />
   </children>
</VBox>
